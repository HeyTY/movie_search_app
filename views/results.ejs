<% include partials/header%>

<h1>Results Page</h1>

<form action="/results" method="GET">
	<input type="text" name="search" placeholder="Enter Movie Title Here" >
	<input type= "submit">
</form>


<ul>
	<% data["Search"].forEach(function(movie) { %>
		<div>
			<img src="<%=movie.Poster%>" alt="" height="175" width="115">
			<strong>
				<%= movie.Title%>
			</strong> 
			- (<%=movie.Year %> )
		</div>
	<%});%>
</ul>

<a href="/">Search Again</a>


<% include partials/footer%>