<% include ../partials/header%>

<div class="container">
	<div class="row">
		<h1>Add New Comments</h1>
		<div>
			<form action="/chart" method="POST">
				<div class="form-group">
					<input class="form-control" type="text" name="movie[title]" placeholder="Comment">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="[year]" placeholder="Author">
				</div>
			
				<div class="form-group">
					<button class="btn btn-lg btn-success btn-block">Submit</button>
				</div>
			</form>
		</div>

		<a href="/chart">Go Back</a>
	</div>
</div>

